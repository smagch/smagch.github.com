
@font-face {
    font-family: 'Ubuntu';
    src: url('/types/ubuntumono-r-webfont.eot');
    src: url('/types/ubuntumono-r-webfont.eot?#iefix') format('embedded-opentype'),
         url('/types/ubuntumono-r-webfont.woff') format('woff'),
         url('/types/ubuntumono-r-webfont.ttf') format('truetype'),
         url('/types/ubuntumono-r-webfont.svg#robotobold') format('svg');
    font-weight: normal;
    font-style: normal;
}

