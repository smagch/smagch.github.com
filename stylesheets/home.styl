@import "lib/types/linux"
@import "lib/types/robot"
@import "lib/types/ubuntu"
@import "variables"
@import "lib/highlight"
@import "lib/nav"
@import "lib/h5bt"
@import "lib/util"
@import "base"

ul.items
  list-style: none
  margin: 0
  padding: 0
  width: 100%
  padding: 0 0 200px
  li
    position: relative
    a
      display: block
      margin: 0 auto
      font-size: 26px
      padding-left: 20px
      line-height: 70px
      height: 60px
      border-bottom: 1px solid darken($main_back, 4%)
      text-decoration: none
    time
      position: absolute
      bottom: 2px
      right: 10px
      font-size: 12px
      color: darken($main_back, 30%)
  h3.center
    margin-top: 80px

@import "lib/h5bt-bottom"
